{
  "name": "create-reusable-react-app",
  "version": "0.2.1",
  "private": false,
  "author": "Mwebaze",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonahgeek/create-reusable-react-app.git"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "fs-extra": "^9.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "setupFiles": [
      "<rootDir>/src/tests/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|styl|less|sass|scss)$": "identity-obj-proxy"
    }
  },
  "bin": {
    "create-reusable-react-app": "./bin/mwebaze.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bugs": {
    "url": "https://github.com/jonahgeek/create-reusable-react-app/issues"
  },
  "homepage": "",
  "devDependencies": {
    "react-scripts": "^3.4.1"
  }
}
